{% if docBlock.node.response and docBlock.node.response.types | join() != 'void' %}

	{% set tagsHTML %}
	{% set returnType = docBlock.node.response.types ? docBlock.node.response.types | route | join('|') | raw %}
	{% set returnDescription = docBlock.node.response.description ? docBlock.node.response.description | markdown | raw %}
	{% set tag = { klass: 'return', items: ['@return', returnType, returnDescription] } %}
	{% include 'parts/docblock/tag/docblock-tag.html.twig' %}
	{% endset %}

	{% set tagList = { klass: 'return', tagsHTML: tagsHTML } %}
	{% include 'parts/docblock/tag/docblock-tag-list.html.twig' %}

{% endif %}
