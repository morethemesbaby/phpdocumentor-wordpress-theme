<?xml version="1.0" encoding="utf-8"?>
<template>
	<author>metamn</author>
	<email>hello@morethemes.baby</email>
	<version>1.0.0</version>
	<copyright>2018 metamn</copyright>
	<description>
		<![CDATA[
			This is a phpDocumentor template for Wordpress themes.
		]]>
	</description>
	<transformations>
		<transformation writer="twig" query="files" source="templates/phpdocumentor-wordpress-theme/file.html.twig" />
		<transformation writer="twig" source="templates/phpdocumentor-wordpress-theme/index.html.twig" artifact="index.html"/>
		<transformation writer="twig" source="templates/phpdocumentor-wordpress-theme/markers.html.twig" artifact="markers.html"/>
		<transformation writer="twig" source="templates/phpdocumentor-wordpress-theme/errors.html.twig" artifact="errors.html"/>
		<transformation writer="twig" source="templates/phpdocumentor-wordpress-theme/deprecated.html.twig" artifact="deprecated.html"/>
	</transformations>
</template>
